<h1>Bulk Discounts</h1>

<%= @bulk_discounts.each do |discount| %>
  <div id="discount_id-<%= discount.id %>">
    <h2><%= link_to "#{discount.id}", merchant_bulk_discount_path(discount.merchant_id, discount) %></h2>
  </div>
    <li>Markdown: <%= discount.markdown %></li>
    <li>Quantity Threshold: <%= discount.threshold %></li>
<% end %>
